<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" verbose="2">
    <groups>
        <run>
            <include name="Regression"/>
        </run>
    </groups>

    <listeners>
        <listener class-name="utilities.listeners.TestListeners"/>
        <listener class-name="utilities.listeners.SuiteListeners"/>
    </listeners>

    <test name="credentials">
        <packages>
            <package name="credentials"/>
        </packages>
    </test>
    <test name="secured">
        <packages>
            <package name="secured"/>
        </packages>
    </test>
    <test name="checks">
        <packages>
            <package name="checks"/>
        </packages>
    </test>
    <test name="AddRemoveElements">
        <packages>
            <package name="AddRemoveElements"/>
        </packages>
    </test>
    <test name="statuscode">
        <packages>
            <package name="statuscode"/>
        </packages>
    </test>
</suite>